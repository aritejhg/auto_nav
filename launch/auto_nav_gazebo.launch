<launch>
  <!-- Arguments -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="slam_methods" default="gmapping" doc="slam type [gmapping, cartographer, hector, karto, frontier_exploration]"/>
  <arg name="move_forward_only" default="false"/>
  <arg name="open_rviz" default="true"/>

  <!-- General launch-script -->
  <include file="$(find auto_nav)/launch/auto_nav.launch">
    <arg name="model" value="$(arg model)"/>
    <arg name="slam_methods" value="$(arg slam_methods)"/>
    <arg name="configuration_basename" value="turtlebot3_lds_2d_gazebo.lua"/>
    <arg name="move_forward_only" value="$(arg move_forward_only)"/>
    <arg name="open_rviz" default="$(arg open_rviz)"/>
  </include>

</launch>